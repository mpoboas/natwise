@startuml DomainModel
'https://plantuml.com/class-diagram
skinparam classAttributeIconSize 0
hide circle
hide methods

class User {
    - name
    - email 
    - phone
    - address
    - password
}

class Account {
    - accountname
    - user
    - balance
    - type
    - openDate
    - status
}

class Transfer {
    - originAccount
    - destinyAccount
    - value
    - date
    - description
}

class Transaction {
    - account
    - type
    - value
    - date
    - description
}

class TransferHistory {
    - account
    - transfers
}

User "1"-"*" Account
Account "*"-"*" Transfer
Account "*"-"1" TransferHistory
Transfer "*"-"1" TransferHistory
Account "*"-"*" Transaction
Transaction "1"-d-"*" TransferHistory

@enduml
